lst = [["hi", "bye"], "hello", "goodbye", [9, 2], 4]
greeting_doubled = map((lambda value: 2 * value),lst)
<--------------------------------------------->
abbrevs = ["usa", "esp", "chn", "jpn", "mex", "can", "rus", "rsa", "jam"]
abbrevs_upper = map((lambda x: x.upper()), abbrevs)
